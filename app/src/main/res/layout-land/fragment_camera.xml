<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="jp.ac.chiba_fjb.x14b_d.maguro.CameraFragment">

    <TextureView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/textureView" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/a"
        android:id="@+id/imageView"
        tools:ignore="ContentDescription" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/mainasuu"
            android:layout_marginRight="15dp"
            android:layout_marginEnd="15dp"
            android:id="@+id/imageZoomOut"
            android:layout_alignBottom="@+id/imageTimer"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:background="@null"
            tools:ignore="ContentDescription,UnknownId" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/modoru"
            android:id="@+id/imageBack"
            android:background="@null"
            android:layout_alignParentTop="true"
            android:layout_alignLeft="@+id/imageREC"
            android:layout_alignStart="@+id/imageREC"
            tools:ignore="ContentDescription" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/rc"
            android:layout_marginLeft="21dp"
            android:layout_marginStart="21dp"
            android:id="@+id/imageREC"
            android:background="@null"
            android:layout_below="@+id/imageBack"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            tools:ignore="ContentDescription" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/t"
            android:id="@+id/imageTimer"
            android:background="@null"
            android:layout_below="@+id/imageREC"
            android:layout_alignLeft="@+id/imageREC"
            android:layout_alignStart="@+id/imageREC"
            android:contentDescription=""
            tools:ignore="ContentDescription" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/guru"
            android:layout_alignParentBottom="true"
            android:layout_alignRight="@+id/imageZoomOut"
            android:layout_alignEnd="@+id/imageZoomOut"
            android:id="@+id/imageriv"
            android:background="@null" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/purasusu"
            android:id="@+id/imageZoomIn"
            android:background="@null"
            tools:ignore="ContentDescription"
            android:layout_above="@+id/imageZoomOut"
            android:layout_alignLeft="@+id/imageZoomOut"
            android:layout_alignStart="@+id/imageZoomOut" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/member"
            android:id="@+id/imageMember"
            android:background="@null"
            android:layout_alignParentTop="true"
            android:layout_alignLeft="@+id/imageZoomIn"
            android:layout_alignStart="@+id/imageZoomIn" />

        <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="12dp"
            android:layout_marginStart="12dp"
            android:id="@+id/textGPS"
            android:layout_below="@+id/textTeamName"
            android:layout_toRightOf="@+id/imageBack"
            android:layout_toEndOf="@+id/imageBack"
            android:layout_marginTop="20dp" />

        <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textTeamName"
            android:layout_above="@+id/imageZoomIn"
            android:layout_toLeftOf="@+id/imageMember"
            android:layout_toStartOf="@+id/imageMember"
            android:layout_marginRight="20dp"
            android:layout_marginEnd="20dp"
            android:layout_marginBottom="28dp" />

    </RelativeLayout>

</FrameLayout>
